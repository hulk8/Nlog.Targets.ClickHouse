<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>net5.0;netstandard2.1</TargetFrameworks>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>NLog target that pushes log messages to ClickHouse </Title>
        <Authors>hulk8</Authors>
        <Description>NLog target that pushes log messages to ClickHouse </Description>
        <PackageProjectUrl>https://github.com/hulk8/Nlog.Targets.ClickHouse</PackageProjectUrl>
        <PackageTags>nlog,clickhouse</PackageTags>
        <AssemblyVersion>1.0.0</AssemblyVersion>
        <FileVersion>1.0.0</FileVersion>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="ClickHouse.Client" Version="4.2.2" />
      <PackageReference Include="NLog" Version="4.7.11" />
    </ItemGroup>

    <ItemGroup>
      <Compile Remove="ClickHouseLoaderV2.cs" />
      <None Include="ClickHouseLoaderV2.cs" />
    </ItemGroup>

</Project>
